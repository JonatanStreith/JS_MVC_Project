@model JS_MVC_Project.Models.PeopleList

@{
    ViewBag.Title = "List Of People";
}

<h2>List Of People</h2>


<table id="People_List">
    @{
        for (int i = 0; i < Model.listing.Count; i++)
        {
    <tr>
        <td>@Model.listing[i].Name</td>
        <td>@Model.listing[i].Phone</td>
        <td>@Model.listing[i].City</td>





    </tr>
        }
    }



</table>
