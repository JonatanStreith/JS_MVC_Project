@model JS_MVC_Project.Models.PersonDataBundle

@{
    int start = Model.TheListStart;
    if(start < 0) { start = 0; }
    int end = Model.TheListStart + 10;
    if(end>Model.TheList.Count) { end = Model.TheList.Count; }


    for(int i = start; i < end; i++) {

        Html.RenderPartial("PV_Person", Model.TheList[i]);

    }


